import unittest
from classes import *

class test_province_count(unittest.TestCase):
    def setUp(self):
        global provnum
        self.assertEqual(Province.total_provinces,provnum)
                     
class TestValidity(unittest.TestCase):
        def test_return_valid(result):
            if result == "OK":
                return "OK"
            else:
                return "NO"

        def test_ok(self):
            self.assertEqual("OK",test_return_valid("OK"))
            return("OK")
        
        def test_no(self):
            self.assertEqual("OK",test_return_valid("NO"))
            return("NO")