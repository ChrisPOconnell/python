import unittest
class TestValidity(unittest.TestCase):
        #def setUp(self):
        #    self.valid=result
        #    #self.count=provnum
        def return_valid(result):
            if result == "OK":
                return "OK"
            else:
                return "NO"
        def test_ok(self):
            self.assertEqual("OK",return_valid("OK"))
            return("OK")
        def test_no(self):
            self.assertEqual("OK",return_valid("NO"))
            return("NO")